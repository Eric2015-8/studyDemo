<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <!-- 销售账单 -->
        <record id="seq_sale_order" model="ir.sequence">
            <field name="name">销售账单</field>
            <field name="code">jc_finance.sale_account</field>
            <field name="prefix">%(year)s/%(month)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- 收款单 -->
        <record id="seq_jc_finance_receipt" model="ir.sequence">
            <field name="name">收款单</field>
            <field name="code">jc_finance.receipt</field>
            <field name="prefix">%(year)s/%(month)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- 付款单 -->
        <record id="seq_jc_finance_payment" model="ir.sequence">
            <field name="name">付款单</field>
            <field name="code">jc_finance.payment</field>
            <field name="prefix">%(year)s/%(month)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- 记账单 -->
        <record id="seq_jc_finance_memo_ticket" model="ir.sequence">
            <field name="name">记账单</field>
            <field name="code">jc_finance.memo_ticket</field>
            <field name="prefix">%(year)s/%(month)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>