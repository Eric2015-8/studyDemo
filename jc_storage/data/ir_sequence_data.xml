<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <!-- 销售出库单 -->
        <record id="seq_sale_order" model="ir.sequence">
            <field name="name">销售出库单</field>
            <field name="code">jc_storage.sale_out_store</field>
            <field name="prefix">%(year)s/%(month)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- 其他入库单 -->
        <record id="seq_other_in_store" model="ir.sequence">
            <field name="name">其他入库单</field>
            <field name="code">jc_storage.other_in_store</field>
            <field name="prefix">%(year)s/%(month)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- 其他出库单 -->
        <record id="seq_other_out_store" model="ir.sequence">
            <field name="name">其他出库单</field>
            <field name="code">jc_storage.other_out_store</field>
            <field name="prefix">%(year)s/%(month)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- 调拨出库 -->
        <record id="seq_transfer_out" model="ir.sequence">
            <field name="name">调拨出库单</field>
            <field name="code">jc_storage.transfer_out</field>
            <field name="prefix">%(year)s/%(month)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- 调拨入库 -->
        <record id="seq_transfer_in" model="ir.sequence">
            <field name="name">调拨入库单</field>
            <field name="code">jc_storage.transfer_in</field>
            <field name="prefix">%(year)s/%(month)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- 销售退库 -->
        <record id="seq_transfer_in" model="ir.sequence">
            <field name="name">销售退库单</field>
            <field name="code">jc_storage.sale_return_store</field>
            <field name="prefix">%(year)s/%(month)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>